    "chart.js": "2.9.3",
    "classnames": "2.2.6",
    "moment": "2.24.0",
    "node-sass": "4.13.1",
    "nouislider": "14.1.1",
    "react": "16.12.0",
    "react-chartjs-2": "2.9.0",
